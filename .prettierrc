{
  "trailingComma": "none",
  "tabWidth": 4,
  "semi": true,
  "printWidth": 120,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "endOfLine": "auto",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*",
      "options": {
        "operatorLinebreak": "before"
      }
    }
  ]
}
